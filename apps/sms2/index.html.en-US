  <body role="application">
    <article id="login">
      <section class="panel" role="region">
        <header>
          <h1 id="unlock-sim-header">Login</h1>
        </header>
        <form id="login-form">
          <label for="user-email">Type your email:</label>
          <input id="user-email" name="user-email" value="willyaranda@gmail.com" type="email">
          <label for="user-password">Type your password:</label>
          <input id="user-password" name="user-password" value="ruydiypncigkpdfb" type="password">
        </form>
        <button id="login-button">
          <span class="icon icon-close">Login</span>
        </button>
            
      </section>
    </article>
    <article id="main-wrapper" class="wrapper">
      <section id="thread-list" class="panel" role="region">
        <header class="view-header regular-header">
          <menu type="toolbar">
            <a href="#new" id="icon-add">
              <span class="icon icon-compose disabled"></span>
            </a>
            <a href="#logout" id="icon-logout">
              <span class="icon icon-close"></span>
            </a>
          </menu>
          <h1>PushApp!</h1>
        </header>
        <article id="thread-list-container" class="view-body" data-type="list">
        </article>
        <div id="threads-fixed-container" class="fixed-title"></div>
        <form id="threads-edit-form" role="dialog" data-type="edit">
          <section>
            <header>
              <button id="threads-cancel-button">
                <span data-l10n-id="cancel" class="icon icon-close">Cancel</span>
              </button>
              <menu type="toolbar">
                <button id="threads-delete-button" data-l10n-id="delete">Delete</button>
              </menu>
              <h1 id="list-edit-title" data-l10n-id="editMode">Edit messages</h1>
            </header>
          </section>
          <menu>
            <button id="deselect-all-threads" class="edit-button disabled" data-l10n-id="deselect-all">Deselect all</button>
            <button id="select-all-threads" class="edit-button" data-l10n-id="select-all">Select all</button>
          </menu>
        </form>
      </section>

      <section id="thread-messages" class="panel" role="region">
        <header class="view-header regular-header">
          <a role="link" id="go-to-threadlist">
            <span class="icon icon-back"></span>
          </a>
          <menu type="toolbar">
            <a id="icon-contact">
              <span class="icon icon-user"></span>
            </a>
          </menu>
          <h1 id="header-text" data-l10n-id="messages" aria-hidden="true">Messages</h1>
          <form id="messages-tel-form">
            <input id="receiver-input" name="tel" class="tel" type="text">
            <span id="clear-search" role="button" class="icon icon-clear"></span>
          </form>
        </header>
        <div id="contact-carrier" data-l10n-id="carrier-unknown" class="carrier-wrapper">Carrier unknown</div>
        <article id="messages-container" class="view-body" data-type="list">
        </article>
        <form role="search" id="new-sms-form" class="bottom new-sms-form">
          <button id="send-message" disabled="" data-l10n-id="send" type="submit">Send</button>
          <p>
            <textarea type="text" id="message-to-send" name="message-to-send" placeholder="Message" data-l10n-id="composeMessage"></textarea>
          </p>
        </form>
        <form role="dialog" id="messages-edit-form" data-type="edit">
          <section>
            <header>
              <button id="messages-cancel-button">
                <span data-l10n-id="cancel" class="icon icon-close">Cancel</span>
              </button>
              <menu type="toolbar">
                <button id="messages-delete-button" data-l10n-id="delete">Delete</button>
              </menu>
              <h1 id="messages-edit-title" data-l10n-id="editMode">Edit messages</h1>
            </header>
          </section>
          <menu>
            <button id="deselect-all-messages" class="edit-button disabled" data-l10n-id="deselect-all">Deselect all</button>
            <button id="select-all-messages" class="edit-button" data-l10n-id="select-all">Select all</button>
          </menu>
        </form>
      </section>
    </article>
    <article id="loading">
      <form role="dialog" data-type="confirm" class="loading-container">
        <section>
          <p class="loading-header">Login...</p>
          <div role="status">
            <progress class="small"></progress>
          </div>
        </section>
      </form>
    </article>
  </body>
